{
  "expo": {
    "name": "NaDaSie.pl - Zakupy ZERO WASTE",
    "slug": "nadasie-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "pl.nadasie.app",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "plugins": [
      "expo-notifications"
    ]
  }
}
